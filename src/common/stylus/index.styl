@import "./base.styl"
@import "./icon.styl"

.clearfix
  zoom 1
  &:after
    display block
    clear both
    content ""

.btn-style
      width 200px
      margin 10px
      fslh(16px, 40px)
      cursor pointer
      border-radius 10px
      background-color #fff
      outline none
      &:hover
        hoverElement(,mainColor)
      &:active
        activeElement()